{
  "kind": "collectionType",
  "collectionName": "collections",
  "info": {
    "singularName": "collection",
    "pluralName": "collections",
    "displayName": "Collections"
  },
  "options": {
    "timestamps": true,
    "privateAttributes": ["id", "created_at", "updated_at"],
    "populateCreatorFields": false,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "versions": {
      "versioned": true
    },
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "model": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "kind": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "brand": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "qty": {
      "type": "integer",
      "min": 1,
      "max": 1000,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "year": {
      "type": "integer",
      "min": 1000,
      "max": 2100,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "collectionDate": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "resource": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "feature": {
      "type": "boolean",
      "default": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "standard": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "provenance": {
      "type": "component",
      "repeatable": true,
      "component": "shared.provenance"
    },
    "exhibitionStatus": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "exhibitionHistory": {
      "type": "component",
      "repeatable": true,
      "component": "shared.exhibition-history"
    },
    "managementNumber": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "size": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "weight": {
      "type": "integer",
      "min": 1,
      "max": 1000,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "exhibition": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::exhibition.exhibition",
      "inversedBy": "collections"
    }
  }
}
